{
    "name": "pusher-code-collab",
    "displayName": "Pusher-Code-Collab",
    "description": "Realtime Code Collaboration",
    "version": "0.0.1",
    "publisher": "Code-Collaborator",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.startWire",
        "onCommand:extension.joinWire"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.startWire",
                "title": "Start Wire"
            },
            {
                "command": "extension.joinWire",
                "title": "Join Wire"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "copy-paste": "^1.3.0",
        "pusher": "^1.5.1",
        "pusher-client": "^1.1.0",
        "pusher-js": "^4.2.1",
        "randomstring": "^1.1.5"
    }
}
